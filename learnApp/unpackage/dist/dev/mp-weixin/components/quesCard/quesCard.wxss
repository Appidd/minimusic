.queList {
  font-size: 28rpx;
}
.queList .outbox {
  display: flex;
  justify-content: center;
  align-items: flex-end;
  flex-direction: column;
  background-color: white;
  margin-top: 20rpx;
  padding: 20rpx 0rpx;
}
.queList .outbox .queItem {
  box-sizing: border-box;
  width: 100%;
  background-color: red;
  border-radius: 20rpx;
  display: flex;
  align-items: center;
  padding: 0rpx 20rpx;
  justify-content: space-between;
}
.queList .outbox .queItem .itemName image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 20rpx;
}
.queList .outbox .quean2 {
  box-sizing: border-box;
  width: 100%;
  align-items: center;
  padding: 0rpx 20rpx;
}
.queList .outbox .quean2 .quean_top {
  display: flex;
  justify-content: space-between;
}
.queList .outbox .quean2 .quean_top .itemName {
  display: flex;
  align-items: center;
}
.queList .outbox .quean2 .quean_top .itemName image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 20rpx;
}
.queList .outbox .quean2 .quean_top .itemnum {
  font-size: 28rpx;
  color: #8D8E99;
  display: flex;
  justify-content: center;
  align-items: center;
  width: auto;
}
.queList .outbox .quean2 .btns {
  display: flex;
  flex-direction: row-reverse;
  margin-top: 30rpx;
}
.queList .outbox .quean2 .btns .btn {
  display: flex;
  align-items: center;
  font-size: 26rpx;
  color: #1B1C33;
  background-color: #F3F5F7;
  width: 140rpx;
  height: 60rpx;
  justify-content: center;
  border-radius: 30rpx;
}
.queList .outbox .quean2 .btns .btn:last-child {
  margin-right: 20rpx;
}
.queList .outbox .quean2 .btns .btn image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}